eng:kick_the_bucket  a   dbnary:Vocable ;
        dbnary:refersTo  eng:kick_the_bucket__Verb__1 .
-----
eng:kick_the_bucket__Verb__1
        a                     lemon:LexicalEntry ;
        dbnary:partOfSpeech   "Verb" ;
        lemon:canonicalForm   [ lemon:writtenRep       "kick the bucket"@en ;
                                lexinfo:pronunciation  "/ˈkɪk ðə ˈbʌkɪt/"@en-fonipa
                              ] ;
        lemon:language        "en" ;
        lemon:sense           eng:__ws_2_kick_the_bucket__Verb__1 , eng:__ws_1_kick_the_bucket__Verb__1 ;
        dcterms:language      lexvo:eng ;
        lexinfo:partOfSpeech  lexinfo:verb .
------
